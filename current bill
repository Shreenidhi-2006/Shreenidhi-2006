//c programmimg for calculation of cureent bill.
#include<stdio.h>
int main()
{
int id,units,tot_unit,extra;
char name;
printf("enter your name:");
scanf("%s",&name);
printf("enter your ID:");
scanf("%d",&id);
printf("enter the amount of units used:");
scanf("%d",&units);
if(units<=199)
{
tot_unit=units*1.20;
printf("The amoutnt should be paid is: %d ",tot_unit);
}
else if(units>199&&units<400)
{
tot_unit=units*1.50;
printf("The amoutnt should be paid is: %d ",tot_unit);
}
else if(units>400&&units<600)
{
tot_unit=units*1.50;
extra=(15\1000)*(tot_unit);
printf("The amoutnt should be paid is: %d ",tot_unit);
}
else
{
tot_unit=units*1.50;
extra=15\1000*(tot_unit);
printf("The amoutnt should be paid is: %d ",tot_unit);
}


}






